  body.hold-transition.skin-blue.sidebar-mini
    .wrapper
      // Header
      header.main-header
        // Logo
        a.logo(href='index.html')
          // mini logo for sidebar mini 50x50 pixels
          span.logo-mini
            img(src='dist/img/shopping-bag-flat.png', alt='', width='45px', height='45px')
          // logo for regular state and mobile devices
          span.logo-lg
            img(src='dist/img/shopping-bag-flat.png', alt='', width='45px', height='45px')
            b shop
            | NAMMAI
        // Header Navbar: style can be found in header.less
        nav.navbar.navbar-static-top
          // Sidebar toggle button
          a.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
            span.sr-only Toggle navigation
          .navbar-custom-menu
            ul.nav.navbar-nav
              // User Account: style can be found in dropdown.less
              li.dropdown.user.user-menu
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  img.user-image(src='dist/img/admin.jpg', alt='User Image')
                  span.hidden-xs Mai Hữu Nam
                ul.dropdown-menu
                  // User image
                  li.user-header
                    img.img-circle(src='dist/img/admin.jpg', alt='User Image')
                    p
                      | Mai Hữu Nam - Web Developer Full Stack
                      small Phone number: 0967-399-470
                  // Menu Body
                  li.user-body
                    .row
                      .col-xs-4.text-center
                        a(href='#') Followers
                      .col-xs-4.text-center
                        a(href='#') Sales
                      .col-xs-4.text-center
                        a(href='#') Friends
                    // /.row
                  // Menu Footer
                  li.user-footer
                    .pull-left
                      a.btn.btn-default.btn-flat(href='#') Profile
                    .pull-right
                      a.btn.btn-default.btn-flat(href='#') Sign out
      // Thanh sidebar
      aside.main-sidebar
        // sidebar: style can be found in sidebar.less
        section.sidebar
          // Sidebar user panel
          .user-panel
            .pull-left.image
              img.img-circle(src='dist/img/admin.jpg', alt='User Image')
            .pull-left.info
              p Mai Hữu Nam
             
          // search form
          form.sidebar-form(action='#', method='get')
            .input-group
              input.form-control(type='text', name='q', placeholder='Search...')
              span.input-group-btn
                button#search-btn.btn.btn-flat(type='submit', name='search')
                  i.fa.fa-search
          // /.search form
          // sidebar menu: : style can be found in sidebar.less
          ul.sidebar-menu
            li.header MAIN NAVIGATION
            // Loại sản phẩm
            li.treeview
              a(href='#')
                i.fa.fa-files-o
                span Loại sản phẩm
                span.pull-right-container
                  span.label.label-primary.pull-right 4
              ul.treeview-menu
                li
                  a(href='#lap-top')
                    i.fa.fa-circle-o
                    | Lap Top
                li
                  a(href='#dien-thoai')
                    i.fa.fa-circle-o
                    | Điện Thoại
                li
                  a(href='#may-tinh-bang')
                    i.fa.fa-circle-o
                    | Máy tính bảng
                li
                  a(href='#phu-kien')
                    i.fa.fa-circle-o
                    | Phụ kiện
                li
                  a(href='users/list')
                    i.fa.fa-list-alt
                    | Danh sách
                li
                  a(href='/users/add')
                    i.fa.fa-plus
                    | Thêm mới
            // Sản phẩm
            li.treeview
              a(href='#')
                i.fa.fa-files-o
                span Sản phẩm
                span.pull-right-container
                  i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  a(href='list.html')
                    i.fa.fa-list-alt
                    | Danh sách
                li
                  a(href='add.html')
                    i.fa.fa-plus
                    | Thêm mới
            // Người dùng
            li.treeview
              a(href='#')
                i.fa.fa-files-o
                span Người dùng
                span.pull-right-container
                  i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  a(href='list.html')
                    i.fa.fa-list-alt
                    | Danh sách
                li
                  a(href='add.html')
                    i.fa.fa-plus
                    | Thêm mới
            // Slide
            li.treeview
              a(href='#')
                i.fa.fa-share
                span Slide
                span.pull-right-container
                  i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  a(href='#')
                    i.fa.fa-circle-o
                    |  Level One
                li
                  a(href='#')
                    i.fa.fa-circle-o
                    |  Level two
                li
                  a(href='#')
                    i.fa.fa-circle-o
                    |  Level three
            li.header Information
            li
              a(href='#')
                i.fa.fa-circle-o.text-red
                span Important
            li
              a(href='#')
                i.fa.fa-circle-o.text-yellow
                span Warning
            li
              a(href='#')
                i.fa.fa-circle-o.text-aqua
                span Information
